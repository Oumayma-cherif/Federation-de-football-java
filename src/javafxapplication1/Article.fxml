<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="468.0" prefWidth="743.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafxapplication1.ArticleController">
   <children>
      <TableView fx:id="taba" layoutX="34.0" layoutY="30.0" prefHeight="267.0" prefWidth="688.0">
        <columns>
          <TableColumn fx:id="id" prefWidth="40.0" text="id" />
          <TableColumn fx:id="user_id" prefWidth="110.0" text="username" />
            <TableColumn fx:id="titre" onEditCommit="#onedittitre" prefWidth="122.0" text="titre" />
            <TableColumn fx:id="descr" onEditCommit="#oneditdescr" prefWidth="210.0" text="descr" />
            <TableColumn fx:id="datea" prefWidth="67.0" text="datea" />
            <TableColumn fx:id="img" prefWidth="131.0" text="img" />
        </columns>
      </TableView>
      <TextField fx:id="tatitre" layoutX="139.0" layoutY="304.0" />
      <TextField fx:id="tadescr" layoutX="139.0" layoutY="356.0" />
      <ComboBox fx:id="user_c" layoutX="436.0" layoutY="312.0" prefWidth="150.0" />
      <Label layoutX="49.0" layoutY="308.0" text="titre" />
      <Label layoutX="44.0" layoutY="369.0" text="descr" />
      <Label layoutX="368.0" layoutY="317.0" text="user" />
      <Button fx:id="upload" layoutX="443.0" layoutY="371.0" mnemonicParsing="false" onAction="#onupload" text="Upload" />
      <Label fx:id="imglabel" layoutX="453.0" layoutY="328.0" />
      <Button fx:id="save" layoutX="283.0" layoutY="398.0" mnemonicParsing="false" onAction="#onsave" text="Save" />
      <Button fx:id="remove" layoutX="653.0" layoutY="316.0" mnemonicParsing="false" onAction="#onremove" text="remove" />
   </children>
</AnchorPane>
