<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="464.0" prefWidth="692.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafxapplication1.P_c_m_cController">
   <children>
      <VBox layoutX="82.0" layoutY="293.0" prefHeight="33.0" prefWidth="199.0" spacing="15.0" />
      <Button fx:id="btndeletecat" layoutX="163.0" layoutY="296.0" mnemonicParsing="false" onAction="#deletecat" prefHeight="25.0" prefWidth="54.0" text="Delete" />
      <VBox layoutX="25.0" layoutY="40.0" prefHeight="25.0" prefWidth="59.0" spacing="15.0">
         <children>
            <Label text="Categorie:" />
         </children>
      </VBox>
      <VBox layoutX="84.0" layoutY="40.0" prefHeight="25.0" prefWidth="147.0">
         <children>
            <TextField fx:id="tfcat" />
         </children>
      </VBox>
      <TableView fx:id="tablecat" layoutX="323.0" layoutY="40.0" onMouseClicked="#catclick" prefHeight="281.0" prefWidth="353.0">
        <columns>
          <TableColumn fx:id="idcat" prefWidth="177.0" text="Id" />
          <TableColumn fx:id="typecat" onEditCommit="#catedit" prefWidth="175.0" text="Type Categorie" />
        </columns>
      </TableView>
      <Button fx:id="btninsertcat" layoutX="90.0" layoutY="296.0" mnemonicParsing="false" onAction="#insertcat" prefHeight="25.0" prefWidth="49.0" text="Insert " />
      <Label fx:id="cateroor" layoutX="25.0" layoutY="78.0" textFill="#ff0101" />
      <Button fx:id="Homebtn" layoutX="320.0" layoutY="398.0" mnemonicParsing="false" onAction="#switchtohome" text="Home" />
   </children>
</AnchorPane>
