<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="700.0" prefWidth="700.0" stylesheets="@bootstrap.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.AffichRewardsController">
   <children>
      <TableView fx:id="tableRewards" layoutX="33.0" layoutY="66.0" prefHeight="500.0" prefWidth="600.0" stylesheets="@bootstrap.css">
        <columns>
          <TableColumn fx:id="id" onEditStart="#supp" prefWidth="75.0" text="id" />
          <TableColumn fx:id="trophe" onEditCommit="#changeTrophe" prefWidth="75.0" text="trophe" />
            <TableColumn fx:id="rank" onEditCommit="#changeRank" prefWidth="75.0" text="rank" />
            <TableColumn fx:id="prixR" onEditCommit="#changePrix" prefWidth="67.0" text="prix" />
            <TableColumn fx:id="img" onEditCommit="#changeImage" prefWidth="183.0" text="image" />
            <TableColumn fx:id="id_tournoi" onEditCommit="#changeTournoi" prefWidth="124.0" text="nom Tournoi" />
        </columns>
      </TableView>
      <Button fx:id="btx" layoutX="613.0" layoutY="586.0" mnemonicParsing="false" onAction="#ajoutReward" styleClass="shopping-online-card" stylesheets="@style.css" text="ajouter" />
      <Button fx:id="back" layoutX="21.0" layoutY="21.0" mnemonicParsing="false" onAction="#backM" styleClass="shopping-online-card" stylesheets="@style.css" text="back" />
   </children>
</AnchorPane>
