            <TableColumn fx:id="id_nbrc" onEditCommit="#changenbrc" prefWidth="75.0" text="nbrc" />
            <TableColumn fx:id="id_img" prefWidth="75.0" text="logo" />
        </columns>
      </TableView>
      <ImageView fx:id="id_log" fitHeight="210.0" fitWidth="81.0" layoutX="690.0" layoutY="54.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="btx" layoutX="697.0" layoutY="279.0" mnemonicParsing="false" onAction="#ajoutT" text="ajouter" />
      <Button fx:id="pdf" layoutX="698.0" layoutY="216.0" mnemonicParsing="false" onAction="#PDF" text="Pdf" />
   </children>
