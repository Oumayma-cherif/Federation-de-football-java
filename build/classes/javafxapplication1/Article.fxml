<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="468.0" prefWidth="743.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafxapplication1.ArticleController">
   <children>
      <TableView fx:id="taba" layoutX="34.0" layoutY="46.0" prefHeight="267.0" prefWidth="688.0">
        <columns>
          <TableColumn fx:id="id" prefWidth="40.0" text="id" />
          <TableColumn fx:id="user_id" prefWidth="110.0" text="username" />
            <TableColumn fx:id="titre" onEditCommit="#onedittitre" prefWidth="122.0" text="titre" />
            <TableColumn fx:id="descr" onEditCommit="#oneditdescr" prefWidth="210.0" text="descr" />
            <TableColumn fx:id="datea" prefWidth="67.0" text="datea" />
            <TableColumn fx:id="img" prefWidth="131.0" text="img" />
        </columns>
      </TableView>
      <TextField fx:id="tatitre" layoutX="139.0" layoutY="322.0" />
      <TextField fx:id="tadescr" layoutX="139.0" layoutY="374.0" />
      <ComboBox fx:id="user_c" layoutX="433.0" layoutY="327.0" prefWidth="150.0" />
      <Label layoutX="50.0" layoutY="323.0" text="titre" />
      <Label layoutX="45.0" layoutY="378.0" text="descr" />
      <Label layoutX="365.0" layoutY="328.0" text="user" />
      <Button fx:id="upload" layoutX="443.0" layoutY="371.0" mnemonicParsing="false" onAction="#onupload" text="Upload" />
      <Label fx:id="imglabel" layoutX="453.0" layoutY="328.0" />
      <Button fx:id="save" layoutX="283.0" layoutY="412.0" mnemonicParsing="false" onAction="#onsave" text="Save" />
      <Button fx:id="remove" layoutX="644.0" layoutY="325.0" mnemonicParsing="false" onAction="#onremove" text="remove" />
      <ImageView fx:id="rollback" fitHeight="39.0" fitWidth="41.0" layoutX="14.0" layoutY="14.0" onMouseClicked="#rollback" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
